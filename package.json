{
  "name": "server",
  "version": "0.0.1",
  "description": "Serverless TypeScript API backend",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "fetch-secrets:dev": "ts-node fetch-secrets.ts dev",
    "fetch-secrets:prod": "ts-node fetch-secrets.ts prod",
    "build": "nest build",
    "ecr:login": "aws ecr get-login-password --profile gamify --region eu-central-1 | docker login --username AWS --password-stdin 635842976991.dkr.ecr.eu-central-1.amazonaws.com",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "./scripts/dev.sh",
    "start:debug": "serverless offline --inspect",
    "debug": "node --inspect-brk -r ts-node/register -r tsconfig-paths/register src/main-dev.ts",
    "debug:brightdata": "node --inspect-brk -r ts-node/register -r tsconfig-paths/register debug-brightdata.ts",
    "debug:brightdata-consumer": "node --inspect-brk -r ts-node/register -r tsconfig-paths/register debug-brightdata-consumer.ts",
    "debug:generate-account-data": "node --inspect-brk -r ts-node/register -r tsconfig-paths/register debug-generate-account-data.ts",
    "start": "nest start --watch src/main-dev.ts",
    "start:dev": "cross-env NODE_ENV=dev dotenv -e .env.dev nest start --watch src/main-dev.ts",
    "start:prod": "cross-env NODE_ENV=prod dotenv -e .env.prod nest start --watch src/main-dev.ts",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "test": "NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy:dev": "npm run fetch-secrets:dev && npm run prisma:deploy:dev && npm run ecr:login && cross-env NODE_ENV=dev dotenv -e .env.dev -- serverless deploy --stage dev",
    "deploy:prod": "npm run fetch-secrets:prod && npm run prisma:deploy:prod && npm run ecr:login && cross-env NODE_ENV=prod dotenv -e .env.prod -- serverless deploy --stage prod --force",
    "local:deploy": "serverless deploy --aws-profile gamify --stage dev",
    "local:deploy:prod": "serverless deploy  --aws-profile gamify --stage prod --force",
    "serverless:offline": "serverless offline",
    "clean": "rm -rf dist node_modules .turbo .webpack layer layer-core layer-db",
    "analyze-deps": "node scripts/analyze-dependencies.js",
    "prisma:generate": "prisma generate",
    "prisma:generatesql": "prisma generate --sql",
    "prisma:format": "prisma format",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:deploy:dev": "cross-env NODE_ENV=dev dotenv -e .env.dev.local prisma migrate deploy",
    "prisma:deploy:prod": "cross-env NODE_ENV=dev dotenv -e .env.prod.local prisma migrate deploy",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:bootstrap": "ts-node src/prisma/prisma-bootstrap.ts",
    "prisma:clean": "ts-node src/prisma/prisma-clean.ts",
    "prisma:seed:ai": "ts-node src/prisma/seed/seed-ai-settings.ts",
    "prisma:seed:users": "ts-node src/prisma/seed/seed-users.ts",
    "prisma:seed:regions": "ts-node src/prisma/seed/seed-regions.ts",
    "prisma:remove:accounts": "ts-node src/prisma/seed/remove-accounts.ts",
    "compose:up": "docker compose -f docker-compose.yml up -d",
    "compose:down": "docker compose -f docker-compose.yml down",
    "seed": "ts-node src/prisma/seed/seed.ts",
    "update:me-in-prompt": "ts-node src/scripts/update-me-in-prompt.ts",
    "send-to-recovery": "ts-node src/scripts/send-to-recovery.ts",
    "debug:send-to-recovery": "node --inspect-brk -r ts-node/register -r tsconfig-paths/register src/scripts/send-to-recovery.ts",
    "update:topics-volume": "ts-node -r tsconfig-paths/register src/scripts/update-topics-volume.ts",
    "debug:update-topics-volume": "node --inspect-brk -r ts-node/register -r tsconfig-paths/register src/scripts/update-topics-volume.ts",
    "add:prompts": "ts-node src/scripts/add-prompts.ts",
    "update:prompts-volume": "ts-node -r tsconfig-paths/register src/scripts/update-prompts-volume.ts",
    "backfill:prompt-sentiment": "ts-node -r tsconfig-paths/register src/scripts/backfill-prompt-sentiment.ts",
    "debug:update-prompts-results-is-branded": "node --inspect-brk -r ts-node/register -r tsconfig-paths/register src/scripts/update-prompts-results-is-branded.ts",
    "update-prompts-results-is-branded": "ts-node -r tsconfig-paths/register src/scripts/update-prompts-results-is-branded.ts",
    "apply:last-insights-visit": "ts-node -r tsconfig-paths/register src/scripts/apply-last-insights-visit-column.ts",
    "create:tracker": "npm run minify:tracker && npm run upload-tracker-to-s3",
    "minify:tracker": "terser ./public/tracker.js -c -m --comments '/Copyright|@preserve|@license/i' -o ./public/tracker.min.js",
    "upload-tracker-to-s3": "ts-node -r tsconfig-paths/register src/scripts/upload-tracker-to-s3.ts",
    "victor:ingest": "ts-node -r tsconfig-paths/register src/embeddings/run-ingestion.ts",
    "victor:clean-reingest": "ts-node -r tsconfig-paths/register src/embeddings/clean-and-reingest.ts"
  },
  "prisma": {
    "seed": "ts-node src/prisma/seed/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.782.0",
    "@aws-sdk/client-secrets-manager": "^3.864.0",
    "@aws-sdk/lib-storage": "^3.782.0",
    "@aws-sdk/s3-request-presigner": "^3.782.0",
    "@descope/node-sdk": "^1.7.7",
    "@getbrevo/brevo": "^3.0.1",
    "@google/generative-ai": "^0.24.1",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.14",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.14",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.1.1",
    "@nestjs/terminus": "^11.0.0",
    "@prisma/client": "^6.6.0",
    "@vendia/serverless-express": "^4.12.6",
    "aws-lambda": "^1.0.7",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^2.2.0",
    "cache-manager": "^7.0.0",
    "cheerio": "^1.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^10.0.0",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.5.2",
    "depd": "^2.0.0",
    "dotenv-cli": "^10.0.0",
    "express": "^5.1.0",
    "geoip-lite": "^1.4.10",
    "jwks-rsa": "^3.2.0",
    "linkedin-api-client": "^0.3.0",
    "localtunnel": "^2.0.2",
    "lodash": "^4.17.21",
    "merge-descriptors": "^2.0.0",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "nestjs-cls": "^6.0.1",
    "nestjs-zod": "^4.3.1",
    "openai": "^4.104.0",
    "@qdrant/js-client-rest": "^1.9.0",
    "passport-jwt": "^4.0.1",
    "pdf-lib": "^1.17.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "stripe": "^18.3.0",
    "swagger-ui-express": "^5.0.1",
    "twitter-api-v2": "^1.25.0",
    "type-fest": "^4.39.1",
    "unsplash-js": "^7.0.20",
    "uuid": "^11.0.3",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@faker-js/faker": "^9.7.0",
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^11.0.4",
    "@nestjs/testing": "^11.0.14",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.18",
    "@testcontainers/postgresql": "^10.28.0",
    "@types/aws-lambda": "^8.10.148",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.1",
    "@types/geoip-lite": "^1.4.4",
    "@types/jest": "^29.5.14",
    "@types/localtunnel": "^2.0.4",
    "@types/lodash": "^4.17.16",
    "@types/ms": "^2.1.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.18.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prisma": "^6.6.0",
    "serverless": "^4.12.0",
    "serverless-domain-manager": "^8.0.0",
    "serverless-offline": "^14.4.0",
    "serverless-webpack": "^5.15.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "testcontainers": "^11.3.0",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
